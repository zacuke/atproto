{
  "name": "@atproto/oauth-provider-ui",
  "version": "0.3.4",
  "license": "MIT",
  "description": "Sign-in & Sign-up UI for the @atproto/oauth-provider",
  "homepage": "https://atproto.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluesky-social/atproto",
    "directory": "packages/oauth/oauth-provider-ui"
  },
  "engines": {
    "node": ">=18.7.0"
  },
  "type": "commonjs",
  "exports": {
    "./bundle-manifest.json": {
      "default": "./dist/bundle-manifest.json"
    },
    "./hydration-data": {
      "types": "./hydration-data.d.ts"
    }
  },
  "files": [
    "dist",
    "hydration-data.d.ts"
  ],
  "optionalDependencies": {
    "@atproto/oauth-provider-api": "workspace:*"
  },
  "devDependencies": {
    "@atproto-labs/fetch": "workspace:*",
    "@atproto-labs/rollup-plugin-bundle-manifest": "workspace:*",
    "@atproto/oauth-provider-api": "workspace:*",
    "@atproto/oauth-scopes": "workspace:*",
    "@atproto/oauth-types": "workspace:*",
    "@hcaptcha/react-hcaptcha": "^1.14.0",
    "@lingui/cli": "^5.5.2",
    "@lingui/core": "^5.5.2",
    "@lingui/react": "^5.5.2",
    "@lingui/swc-plugin": "^5.6.1",
    "@lingui/vite-plugin": "^5.5.2",
    "@tailwindcss/vite": "^4.1.16",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "clsx": "^2.1.1",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^5.0.0",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "scripts": {
    "i18n:extract": "lingui extract --clean",
    "i18n:compile": "lingui compile --typescript",
    "i18n": "pnpm i18n:extract && pnpm i18n:compile",
    "prebuild": "pnpm run i18n:compile",
    "build": "vite build --emptyOutDir -- ignore additional npm args",
    "dev:ui": "vite --port 5174",
    "dev:src": "vite build --watch"
  }
}
